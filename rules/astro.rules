//var Boolean isNight = false

rule "sunrise"
when
    Channel 'astro:sun:home:rise#event' triggered START
then
{    
    vIsNight = false
    logInfo("RULE", "sunrise")
}
end

rule "sunset"
when
    Channel 'astro:sun:home:set#event' triggered START
then
{    
    vIsNight = true
    logInfo("RULE", "sunset")
}
end
